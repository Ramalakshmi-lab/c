#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    int a=0,b=1,c,d=0;
    if(n==a or n==b)
    {
        printf("True");
    }
    c=a+b;
    while(c<=n)
    {
         if(c==n)
        {
            d=1;
            break;
        }
        a=b;
        b=c;
        c=a+b;
    }
    if(d==1)
    {
        printf("True");
    }
    else
    {
        printf("False");
    }
}
